{
  "name": "@packages/types",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "@packages/source": "./src/index.ts",
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      }
    },
    "./*": {
      "import": {
        "@packages/source": "./src/*.ts",
        "types": "./dist/src/*.d.ts",
        "default": "./dist/src/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.4",
    "effect": "^3.16.7"
  },
  "devDependencies": {
    "typescript": "catalog:typescript"
  }
}
