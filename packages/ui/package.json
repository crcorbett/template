{
  "name": "@packages/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "@packages/source": "./src/index.ts",
        "default": "./dist/src/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/src/*.d.ts",
        "@packages/source": "./src/*.ts",
        "default": "./dist/src/*.js"
      }
    },
    "./styles/*.css": "./src/styles/*.css"
  },
  "scripts": {
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@base-ui/react": "catalog:ui",
    "@fontsource-variable/outfit": "catalog:ui",
    "@tabler/icons-react": "catalog:ui",
    "class-variance-authority": "catalog:utils",
    "clsx": "catalog:utils",
    "react": "catalog:react",
    "shadcn": "catalog:ui",
    "tailwind-merge": "catalog:utils",
    "tw-animate-css": "catalog:ui"
  },
  "devDependencies": {
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "tailwindcss": "catalog:build",
    "typescript": "catalog:typescript"
  }
}
