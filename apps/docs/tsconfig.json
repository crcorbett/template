{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "skipLibCheck": true,
    "paths": {
      "$/*": ["./src/*"],
      "@/*": ["./src/*"],
      "fumadocs-mdx:collections/*": ["./.source/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".source/**/*.ts"]
}
